{
  "npmScope": "full-stack",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": ["api"]
    },
    "api-auth": {
      "tags": ["api", "auth"]
    },
    "api-core": {
      "tags": ["api", "core"]
    },
    "api-reports": {
      "tags": ["api", "reports"]
    },
    "fe": {
      "tags": ["fe"]
    },
    "fe-auth": {
      "tags": ["fe", "auth"]
    },
    "fe-core": {
      "tags": ["fe", "core"]
    },
    "fe-e2e": {
      "tags": [],
      "implicitDependencies": ["fe"]
    },
    "fe-main": {
      "tags": ["fe", "main"]
    },
    "fe-ui": {
      "tags": ["fe", "ui"]
    },
    "interfaces": {
      "tags": ["fe", "api"]
    }
  }
}
